{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/api/v1/user/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/api/v1/bank/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  